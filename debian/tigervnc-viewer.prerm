#!/bin/sh
set -e

if [ "$1" = "remove" ] ; then
  bindir=/usr/bin
  mandir=/usr/share/man

  for tiger in xtigervncviewer; do
    notiger=`echo $tiger | sed -e 's/tiger//'`;
    if [ -f $bindir/$tiger ]; then
      update-alternatives --remove $notiger $bindir/$tiger
    fi
  done
fi

#DEBHELPER#

exit 0
